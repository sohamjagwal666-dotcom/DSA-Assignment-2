#include <iostream>
using namespace std;
int main() {
    int n=7;
    int arr[n]={12,13,15,16,17,18,19};
   int start=0;
   int end=n-1;
   int x=15;
    for (int i=0;i<n;i++){
        if(arr[i]!=arr[0]+i){
            cout<<"missing element is "<<arr[0]+i;
            break;
        }
    }
        
}
