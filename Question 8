#include <iostream>
using namespace std;
int main(){
    int arr[10]={2,3,7,5,9,1,2,4,9,3};
    

int arr1[10];
int x=0;
int n=0;
int y=1;
    for(int i=0;i<sizeof(arr)/sizeof(int);i++){
        bool sa=true;
        for(int j=0;j<i;j++){
            if(arr[i]==arr[j]){
                sa=false;
                break;
            }
           
        }
        if (sa==true){
            n++;
        }

        


    }
    cout<<"total number of distinct elements are "<<n;
}
