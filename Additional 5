#include <iostream>
using namespace std;
void shift(int arr[],int i,int n){
    for(int j=n-1;j>i;j--){
        arr[j]=arr[j-1];
    }
}
int main(){
    int n=10;
    int arr[10]={1,2,4,5,6,2,3,7,2,8};
    int x=0;
    for(int i=0;i<n;i++){
        cout<<arr[i];
    }
    cout<<endl;
    while(true){
        if(x>n){
            break;
        }
        else if(arr[x]==2){
            shift(arr,x,n);
            arr[x+1]=2;
            x+=2;
        }
        else{
            x++;
        }
    }
    for(int i=0;i<n;i++){
        cout<<arr[i];
    }
    
}
